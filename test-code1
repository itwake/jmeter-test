import java.util.*;

public class Solution {
    public static int totalLuckyCustomer(int[] list_input, int K) {
        int answer = 0;
        // 创建一个哈希集合来存储已经访问的元素
        HashSet<Integer> seen = new HashSet<>();
        
        // 遍历每个价格
        for (int price : list_input) {
            // 检查是否存在 (price - K) 或 (price + K)
            if (seen.contains(price - K)) {
                answer++;
            }
            if (seen.contains(price + K)) {
                answer++;
            }
            // 将当前价格加入集合
            seen.add(price);
        }
        
        return answer;
    }

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        
        // 输入数组大小
        int list_input_size = in.nextInt();
        int[] list_input = new int[list_input_size];
        
        // 输入产品价格
        for (int i = 0; i < list_input_size; i++) {
            list_input[i] = in.nextInt();
        }
        
        // 输入目标差值 K
        int K = in.nextInt();
        
        // 调用方法并输出结果
        int result = totalLuckyCustomer(list_input, K);
        System.out.println(result);
        
        in.close();
    }
}
